{
  "presets": [
    ["@babel/preset-env", { "useBuiltIns": "usage", "corejs": 2 }],
    "@babel/preset-react"
  ],
  "plugins": [
    // MobX requires legacy decorators as of 2019-04-17.
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
